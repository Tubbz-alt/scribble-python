module popl14.WF;


global protocol ChoiceAmbiguous(role A, role B, role C) {
	choice at A {
		m1() from A to B;
		m2() from B to C;
		m3() from C to A;
	} or {
		m1() from A to B;
		m5() from B to C;
		m6() from C to A;
	}
}


